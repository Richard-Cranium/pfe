$libtool_flags --no-verify $ac_aux_dir/ltmain.sh $lt_target \
|| { { echo "$as_me:3303: error: libtool configure failed" >&5
echo "$as_me: error: libtool configure failed" >&2;}
   { (exit 1); exit 1; }; }

