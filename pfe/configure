  if test -f $ac_prefix_conf_INP ; then
      case $ac_prefix_conf_OUT in
    */*) P=` $as_expr X$ac_prefix_conf_OUT : 'X\(.*[^/]\)//*[^/][^/]*/*$' \| \
         X$ac_prefix_conf_OUT : 'X\(//\)[^/]' \| \
